{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "watch": "rm -rf dist/ && node_modules/webpack/bin/webpack.js --config webpack.dev.config.js --watch",
    "build-prod": "rm -rf dist/* && node_modules/webpack/bin/webpack.js --config webpack.prod.config.js && du -sh dist/",
    "deploy": "git checkout deploy && git pull origin develop && npm run build-prod && git add dist && git commit -m 'deploy' && git push origin deploy && git push heroku deploy:master && git checkout develop"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.3.5",
    "@angular/cdk": "^2.0.0-beta.11",
    "@angular/material": "2.0.0-beta.11",
    "@angular/common": "^4.3.5",
    "@angular/compiler": "^4.3.5",
    "@angular/compiler-cli": "^4.3.5",
    "@angular/core": "^4.3.5",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.3.5",
    "@angular/http": "^4.3.5",
    "@angular/platform-browser": "^4.3.5",
    "@angular/platform-browser-dynamic": "^4.3.5",
    "@angular/platform-server": "^4.3.5",
    "@angular/router": "^4.3.5",
    "@swimlane/ngx-charts": "^6.0.2",
    "@types/node": "^8.0.2",
    "awesome-typescript-loader": "^3.1.3",
    "d3": "^4.10.2",
    "hammerjs": "^2.0.8",
    "purecss": "^1.0.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.34",
    "@types/node": "^7.0.12",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.0",
    "html-webpack-plugin": "^2.28.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.5.1",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.5.5"
  },
  "repository": {}
}
